{
    "env": {
        "browser": true,
        "worker": true
    },
    "extends": [],
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "linebreak-style": ["error", "unix"],
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "no-console": "off"
    },
    "globals": {
        "$": true
    }
}
